    "build:docs": "vue-cli-service build",
    "build": "npm run build:docs",
    "build:js": "node cmd/buildComponents.js",
    "build:css": "node cmd/buildStyle.js",
    "build:single-css-entry": "node cmd/buildSingleCompStyleEntry.js",
    "build:css-entry": "node cmd/buildStyleEntry.js",
    "build:lib": "npm run build:js && npm run build:css && npm run build:css-entry",
    "lint": "eslint \"./docs/\" \"./packages/\" --fix --ext .ts,.tsx,.vue"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{packages,docs}/**/*.{ts,tsx,js,vue}": [
      "eslint --fix",
      "git add ."
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/didengren/zview.git"
  },
  "keywords": [
    "vue",
    "component"
